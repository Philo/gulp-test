{
  "appSassPath": [
    "_/src/sass/*.scss",
    "_/src/sass/**/*.scss"
  ],
  "appJsPath": [
    "_/src/js/*.js",
    "_/src/js/**/**/*.js"
  ],
  "extras": [
    "_/src/**/*",
    "!_/src/{sass,sass/**}",
    "!_/src/{js,js/**}",
    "!_/src/{images,images/**}"
  ],
  "images": [
    "_/src/images/**/*.{png,jpg,jpeg,gif,svg}",
    "_/src/images/*.{png,jpg,jpeg,gif,svg}"
  ],
  "outputPath": {
    "dist" : "_/dist",
    "appSass" : "_/dist/css",
    "appJs" : "_/dist/js",
    "images" : "_/dist/images"
  }
}